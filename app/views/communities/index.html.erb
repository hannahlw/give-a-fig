<div class="container">
<h1>All Communities</h1>
<ul>
<% if user_signed_in? %>
  <% @communities.each do |community| %>
    <li><%= link_to community.name, community_path(community) %> -
    <%= community.admin.first_name %>
    </li>
  <% end %>
<% end %>
</ul>
</div>
<div class="entire-footer">
  <%= render :partial => "layouts/footer" %>
</div>