<%= form_for @community do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br>

  <%= f.label :pickup_site %>:
  <%= f.text_field :pickup_site %><br>

  <%= f.label :tag %>:
  <%= f.text_field :tag %><br>

  <%= f.hidden_field :admin_id, value: current_user.id %>

  <%= f.submit %>
<% end %>
